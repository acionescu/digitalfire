                               "r2" : "C.A == 1 && (RP.X*RP.X + RP.Y*RP.Y) >= 900 ->  OFF; V.CR=['0','0','i','0','i','i','1','0','i']; SCR(V.CR); SNM([1,1,1,0,1,1,1,0]);",

                             "rop": "C.A > 1 -> CHANGE; RNM(1);"
		}
